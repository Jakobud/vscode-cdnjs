{
  "name": "vscode-cdnjs",
  "displayName": "cdnjs",
  "description": "Embed libraries from cdnjs.com in Visual Studio Code",
  "version": "0.1.0",
  "publisher": "JakeWilson",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cdnjs.search",
    "onCommand:cdnjs.recentLibraries"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "cdnjs.search",
      "title": "Search for libraries",
      "category": "cdnjs"
    }, {
      "command": "cdnjs.recentLibraries",
      "title": "Recent libraries",
      "category": "cdnjs"
    }]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "bluebird": "^3.4.6",
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "copy-paste": "^1.3.0",
    "open": "0.0.5",
    "request": "^2.74.0",
    "typings": "^1.3.3"
  }
}